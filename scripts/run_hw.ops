import("rtt_ros")
ros.import("rtt_lwr_cart_ctrl")
loadComponent("cart_ctrl","lwr::RttLwrCartCtrl")
setActivity("cart_ctrl",0.001,LowestPriority,ORO_SCHED_RT)
connectPeers("cart_ctrl","lwr")
connectPeers("cart_ctrl","ft_sensor")
